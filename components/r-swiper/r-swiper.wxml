<!--components/r-swiper/r-swiper.wxml-->
<swiper class="swiper"
        circular
        autoplay
        interval="2000"
        indicator-dots	
        indicator-color="#ff5777">
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image src="{{item.image}}" mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>
